ALGORITHM arrays
VAR
    arr : ARRAY_OF : INTEGER[10];
    sum,i : INTEGER
BEGIN
    FOR i FROM 0 TO 9 DO
        read(arr[i]);
    END_FOR
    sum := 0;
    FOR i FROM 0 TO 9 DO
        sum := sum + arr[i];
    END_FOR
    write("sum=",sum);
END



